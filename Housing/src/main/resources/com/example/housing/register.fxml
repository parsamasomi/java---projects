<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #121273;" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.housing.Register">
   <children>
      <Label layoutX="45.0" layoutY="32.0" text="Register" textFill="WHITE">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <TextField fx:id="txtFirstName" layoutX="120.0" layoutY="110.0" />
      <TextField fx:id="txtID" layoutX="120.0" layoutY="174.0" />
      <TextField fx:id="txtLastName" layoutX="384.0" layoutY="110.0" />
      <Label layoutX="45.0" layoutY="113.0" text="first name :" textFill="WHITE">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="txtPassword" layoutX="384.0" layoutY="174.0" />
      <Label layoutX="309.0" layoutY="113.0" text="last name :" textFill="WHITE">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label layoutX="45.0" layoutY="177.0" text="username :" textFill="WHITE">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label layoutX="310.0" layoutY="177.0" text="password :" textFill="WHITE">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label fx:id="lblRegisterError" layoutX="318.0" layoutY="319.0" text="please complete the fields correctly !" textFill="RED" visible="false">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Button fx:id="btnSignUp" layoutX="58.0" layoutY="317.0" mnemonicParsing="false" onAction="#pressBtnSignUp" text="sign up">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
